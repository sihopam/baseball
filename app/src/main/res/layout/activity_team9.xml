<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="700dp"
        android:layout_gravity="bottom|center">

        <ImageButton
            android:layout_width="155dp"
            android:layout_height="227dp"
            android:id="@+id/Button91"
            android:layout_gravity="left|top"
            android:background="@drawable/heogyungmin9"
            android:onClick="Button91"
            android:layout_marginLeft="30dp" />

        <ImageButton
            android:layout_width="155dp"
            android:layout_height="227dp"
            android:id="@+id/Button92"
            android:layout_gravity="center_horizontal|top"
            android:background="@drawable/jungsubin9"
            android:onClick="Button92" />

        <ImageButton
            android:layout_width="155dp"
            android:layout_height="227dp"
            android:id="@+id/Button93"
            android:layout_gravity="right|top"
            android:background="@drawable/kimhyunsoo9"
            android:onClick="Button93"
            android:layout_marginRight="30dp" />

        <ImageButton
            android:layout_width="155dp"
            android:layout_height="227dp"
            android:id="@+id/Button94"
            android:layout_gravity="left|center_vertical"
            android:background="@drawable/kimjaeho9"
            android:onClick="Button94"
            android:layout_marginLeft="30dp" />

        <ImageButton
            android:layout_width="155dp"
            android:layout_height="227dp"
            android:id="@+id/Button95"
            android:layout_gravity="center"
            android:background="@drawable/koyoungmin9"
            android:onClick="Button95" />

        <ImageButton
            android:layout_width="155dp"
            android:layout_height="227dp"
            android:id="@+id/Button96"
            android:layout_gravity="right|center_vertical"
            android:background="@drawable/minbyungheon9"
            android:onClick="Button96"
            android:layout_marginRight="30dp" />

        <ImageButton
            android:layout_width="155dp"
            android:layout_height="227dp"
            android:id="@+id/Button98"
            android:layout_gravity="center_horizontal|bottom"
            android:background="@drawable/romero9"
            android:onClick="Button98" />

        <ImageButton
            android:layout_width="155dp"
            android:layout_height="227dp"
            android:id="@+id/Button99"
            android:layout_gravity="right|bottom"
            android:background="@drawable/yangeuiji9"
            android:onClick="Button99"
            android:layout_marginRight="30dp" />

        <ImageButton
            android:layout_width="155dp"
            android:layout_height="227dp"
            android:id="@+id/Button97"
            android:layout_gravity="left|bottom"
            android:background="@drawable/ohjaewon9"
            android:onClick="Button97"
            android:layout_marginLeft="30dp" />

    </FrameLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="74dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="두산 베어스"
        android:id="@+id/textView10"
        android:layout_gravity="center_horizontal|top"
        android:layout_marginTop="30dp"
        android:textSize="50dp" />

</FrameLayout>